function vr96_setATW(){try{null!=nume_atw_url&&(jQuery(".nume-vr96 .recos li").off(),jQuery(".nume-vr96 .recos li").on("mouseover",vr96_mover),jQuery(".nume-vr96 .recos li").on("mouseout",vr96_mout),jQuery(".nume-vr96 .atw").off(),jQuery(".nume-vr96 .atw").on("click",vr96_atw))}catch(a){}}function vr96_mover(){jQuery(".atw",this).show()}function vr96_mout(){jQuery(".atw",this).hide()}
function vr96_updateATW(a,b,c){if(!0==c)jQuery(a).removeClass("btn-m btn-ter").addClass("atw-watched").html(b);else{jQuery(a).html(b);var e=setInterval(function(){clearInterval(e);jQuery(a).html(nume_atw_msg)},2E3)}}
function vr96_atw(){var a=jQuery(this).data("itemid"),b=nume_atw_url+a+("&_trksid=c"+nume_pid+".m2510.l5037"),c=jQuery(this);jQuery.ajax({dataType:"json",headers:{srt:nume_atw_token,"Content-Type":"application/json"},url:b}).done(function(b){var b=jQuery.parseJSON(b),d=!1;null!=b.error&&"auth_required"==b.error&&(d=!0);b=b.result;!0==d?window.location.href=nume_atw_url_noajax+a+"&srt="+nume_atw_noajax_token:1==b||2==b||5==b?(vr96_updateATW(c,nume_atw_success,!0),c.off()):3==b?vr96_updateATW(c,nume_atw_full,
!1):vr96_updateATW(c,nume_atw_error,!1)}).fail(function(){window.location.href=nume_atw_url_noajax+a+"&srt="+nume_atw_noajax_token})}function vr96_resizeContainer(){jQuery(".nume-vr96").each(function(){var a=jQuery(".recos li",this).outerWidth(!0)*jQuery(".recos li",this).length;jQuery(".recos",this).width(a+100)})}
function vr96_resetColumns(){jQuery(".nume-vr96").each(function(){var a=jQuery(".center",this).width(),b=jQuery(".recos li",this).outerWidth(),c=Math.floor(a/(b+10)),a=((a-c*b-10)/(c-1)/2).toString()+"px";jQuery(".nume-vr96 .recos li").css({"margin-left":a,"margin-right":a});jQuery(".nume-vr96").each(function(){jQuery(".recos",this).each(function(){jQuery("li",this).first().css({"margin-left":"10px"});jQuery("li",this).last().css({"margin-right":"10px"})});vr96_loadImg(this,c)})})}
function vr96_loadImg(a,b){jQuery(".recos li",a).each(function(a){a<b?(jQuery(".imgt img",this).attr("src",jQuery(this).data("img")),jQuery(this).show()):jQuery(this).hide()})}function vr96_toggleInfo(a){jQuery(".numeinfo",a.data.target).toggle()}
function vr96_init(){vr96_resizeDone();jQuery(".nume-vr96").each(function(){jQuery("h2 .info",this).on("click",{target:this},vr96_toggleInfo);jQuery("h2 .info",this).on("keypress",function(a){13==a.which&&this.click()});jQuery(".numeinfo .close",this).on("click",{target:this},vr96_toggleInfo);jQuery(".numeinfo .close",this).on("keypress",function(a){13==a.which&&this.click()});var a=$("h2 .info",this).position();$(".numeinfo",this).css({top:a.top-33+"px",left:a.left+15+"px"})})}
jQuery(document).ready(function(){try{vr96_setATW(),vr96_init()}catch(a){}});function vr96_resizeDone(){vr96_resetColumns();vr96_resizeContainer()}var vr96_resize;jQuery(window).resize(function(){try{clearTimeout(vr96_resize),vr96_resize=setTimeout(vr96_resizeDone,100)}catch(a){}});