function vr140_scroll(c){var c=c.data.direction,d=jQuery(this).parents(".nume-vr140"),d=jQuery(".nume-vr140").index(d),a=jQuery(this).parents(".nume-vr140");jQuery(".center",this).width();jQuery(".recos li",this).outerWidth();var d=vr140_curr_si[d]*jQuery(".recos li:nth-child(2)",a).outerWidth(!0),b=jQuery(a).index(".nume-vr140"),e=vr140_pos[b];"right"==c?(1==vr140_pos[b]&&(jQuery(".arrow-left-disabled",a).addClass("arrow-left").removeClass("arrow-left-disabled"),jQuery(".arrow-left",a).on("click",
