function vr140_setCounters(c,d){jQuery(".nume-vr140").each(function(a){1!=d[a]?(jQuery(".pgn",this).html(c[a]+"/"+d[a]),jQuery(".arrow-left-disabled",this).show(),jQuery(".arrow-right",this).show()):(jQuery(".pgn",this).html(""),jQuery(".arrow-left",this).hide(),jQuery(".arrow-right",this).hide(),jQuery(".arrow-left-disabled",this).hide(),jQuery(".arrow-right-disabled",this).hide())})}function vr140_mover(){jQuery(".sms",this).css({visibility:"visible"})}
function vr140_mout(){jQuery(".sms",this).css({visibility:"hidden"})}function vr140_loadImg(c,d,a){jQuery(".recos",c).each(function(){jQuery("li",this).each(function(b){b<d*a&&jQuery(".imgt img",this).attr("src",jQuery(this).data("img"))})})}
function vr140_setVisibility(c,d,a,b,e){var f=d;f>a&&(f-=2,f=vr140_enforceMinValue(f,0),a=f+1);jQuery(".recos",c).each(function(){jQuery("li",this).each(function(c){"show"==e?f*b<=c&&c<a*b&&jQuery(this).css({visibility:"visible"}):"hide"==e&&(f*b>c||c>=a*b?jQuery(this).css({visibility:""}):jQuery(this).css({visibility:"visible"}))})})}function vr140_enforceMinValue(c,d){c<d&&(c=d);return c}
function vr140_init(){vr140_curr_si=[];vr140_pos=[];vr140_max=[];vr140_is_init=!1;try{var c=setInterval(function(){!0==vr140_is_init?clearInterval(c):(jQuery(".nume-vr140").each(function(a){vr140_pos[a]=1;vr140_max[a]=1}),vr140_curr_si=vr140_resetColumns(vr140_curr_si),vr140_resizeContainer(),vr140_max=vr140_resetScroll(vr140_pos,vr140_curr_si),jQuery(".nume-vr140 .recos li").off(),jQuery(".nume-vr140 .recos li").on("mouseover",vr140_mover),jQuery(".nume-vr140 .recos li").on("focus",function(){var a=
this;jQuery(".nume-vr140 .recos li").each(function(){a!=this&&jQuery(this).mouseout()});jQuery(this).mouseover()}),jQuery(".nume-vr140 .recos li").on("mouseout",vr140_mout),void 0!=jQuery(".nume-vr140 .recos .imgt img").eq(0).attr("src")&&(vr140_is_init=!0))},200)}catch(d){}}var vr140_curr_si=[],vr140_pos=[],vr140_max=[],vr140_is_init=!1;function vr140_resizeDone(){vr140_curr_si=vr140_resetColumns(vr140_curr_si);vr140_resizeContainer();vr140_max=vr140_resetScroll(vr140_pos,vr140_curr_si)}var vr140_resize;
